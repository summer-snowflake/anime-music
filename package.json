{
  "name": "anime-music",
  "version": "1.0.0",
  "description": "アニメのOPとEDをまとめた情報サイト（の予定）",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "./app/assets/javascripts/jest-script-preprocessor",
    "automock": true,
    "unmockedModulePathPatterns": [
      "react",
      "enzyme"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(coffee|cjsx|js|jsx)$",
    "moduleFileExtensions": [
      "coffee",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/summer-snowflake/anime-music.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/summer-snowflake/anime-music/issues"
  },
  "homepage": "https://github.com/summer-snowflake/anime-music#readme",
  "devDependencies": {
    "babel-jest": "^17.0.0",
    "browserify": "^13.1.0",
    "browserify-incremental": "^3.1.1",
    "coffee-react": "^5.0.1",
    "coffee-react-transform": "^5.0.0",
    "coffeeify": "^2.0.1",
    "coffeelint": "^1.16.0",
    "enzyme": "^2.5.1",
    "jest": "^16.0.2",
    "jest-cli": "^16.0.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "reactify": "^1.1.1"
  }
}
